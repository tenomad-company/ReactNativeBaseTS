{
  "name": "react-native-ts",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:dev": "react-native run-android --variant=developmentDebug --appIdSuffix dev",
    "android:dev-release": "react-native run-android --variant=developmentRelease --appIdSuffix dev",
    "android:staging": "react-native run-android --variant=stagingDebug --appIdSuffix staging",
    "android:staging-release": "react-native run-android --variant=stagingRelease --appIdSuffix staging",
    "android:prod": "react-native run-android --variant=productionDebug",
    "android:prod-release": "react-native run-android --variant=productionRelease",
    "android:build:dev": "cd android && ./gradlew assembleDevelopmentRelease && open ./app/build/outputs/apk/development/release && cd ..",
    "android:build:staging": "cd android && ./gradlew assembleStagingRelease && open ./app/build/outputs/apk/staging/release && cd ..",
    "android:build:prod": "cd android && ./gradlew assembleProductionRelease && open ./app/build/outputs/apk/production/release && cd ..",
    "ios": "react-native run-ios",
    "ios:dev": "react-native run-ios --simulator='iPhone 14 Pro' --scheme 'RentCarDev'",
    "ios:prod": "react-native run-ios --simulator='iPhone 14 Pro' --scheme 'RentCarProd'",
    "ios:staging": "react-native run-ios --simulator='iPhone 14 Pro' --scheme 'RentCarStg'",
    "start": "react-native start",
    "start:reset": "watchman watch-del-all && yarn start --reset-cache",
    "format": "npm run format:js && npm run format:json && npm run format:md && npm run format:ts",
    "format:js": "prettier --write \"app/**/*.js\"",
    "format:json": "prettier --write \"app/**/*.json\"",
    "format:md": "prettier --write \"**/*.md\"",
    "format:ts": "prettier --write \"app/**/*.ts{,x}\"",
    "patch": "patch-package",
    "adb": "adb reverse tcp:9090 tcp:9090 && adb reverse tcp:3000 tcp:3000 && adb reverse tcp:9001 tcp:9001 && adb reverse tcp:8081 tcp:8081",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "android-clean": "cd android && ./gradlew clean",
    "postinstall": "node ./bin/postInstall",
    "linkfont": "react-native link && ./bin/setupAndroidFonts",
    "ios:deploy:prod": "fastlane ios deploy --env prod",
    "ios:deploy:dev": "fastlane ios deploy --env dev",
    "android:deploy:dev": "fastlane android deploy --env dev",
    "android:deploy:prod": "fastlane android deploy --env prod",
    "android:bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.7",
    "@react-navigation/bottom-tabs": "^6.4.0",
    "@react-navigation/material-top-tabs": "^6.2.1",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/native-stack": "^6.6.2",
    "@reduxjs/toolkit": "^1.8.2",
    "axios": "^0.27.2",
    "i18next": "^22.0.2",
    "lottie-react-native": "^5.1.4",
    "native-base": "3.4.0",
    "react": "17.0.2",
    "react-i18next": "^12.0.0",
    "react-native": "0.68.2",
    "react-native-animatable": "^1.3.3",
    "react-native-config": "^1.4.6",
    "react-native-flipper": "^0.164.0",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-permissions": "^3.6.0",
    "react-native-reanimated": "^2.11.0",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^3.13.1",
    "react-native-svg": "^12.1.1",
    "react-native-tab-view": "^3.1.1",
    "react-native-vector-icons": "^9.1.0",
    "react-redux": "^8.0.2",
    "redux-flipper": "^2.0.2",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.67.3",
    "@types/react-native-vector-icons": "^6.4.11",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.32.0",
    "husky": "^8.0.1",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.67.0",
    "patch-package": "^6.4.7",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "lint-staged": {
    "*.+(ts|tsx)": [
      "yarn lint",
      "yarn format:ts"
    ]
  }
}
